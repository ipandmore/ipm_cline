<T3DataStructure>
	<sheets>
		<main>
			<ROOT>
				<TCEforms>
					<label>LLL:EXT:ipm_cline/Resources/Private/Language/locallang.xlf:flexform.main</label>
				</TCEforms>
				<type>array</type>
				<el>
					<settings.mergeFieldsAsAOneLiner>
						<TCEforms>
							<label>LLL:EXT:ipm_cline/Resources/Private/Language/locallang.xlf:flexform.mergeFieldsAsAOneLiner</label>
							<config>
								<type>check</type>
							</config>
						</TCEforms>
					</settings.mergeFieldsAsAOneLiner>
					<settings.startWithCopyrightLabel>
						<TCEforms>
							<label>LLL:EXT:ipm_cline/Resources/Private/Language/locallang.xlf:flexform.startWithCopyrightLabel</label>
							<config>
								<type>check</type>
							</config>
						</TCEforms>
					</settings.startWithCopyrightLabel>
					<settings.copyrightStart>
						<TCEforms>
							<label>LLL:EXT:ipm_cline/Resources/Private/Language/locallang.xlf:flexform.copyrightStart</label>
							<config>
								<type>input</type>
								<eval>date</eval>
							</config>
						</TCEforms>
					</settings.copyrightStart>
					<settings.copyrightEnd>
						<TCEforms>
							<label>LLL:EXT:ipm_cline/Resources/Private/Language/locallang.xlf:flexform.copyrightEnd</label>
							<config>
								<type>input</type>
								<eval>date</eval>
							</config>
						</TCEforms>
					</settings.copyrightEnd>
					<settings.copyrightEndTilNow>
						<TCEforms>
							<label>LLL:EXT:ipm_cline/Resources/Private/Language/locallang.xlf:flexform.copyrightEndTilNow</label>
							<config>
								<type>check</type>
							</config>
						</TCEforms>
					</settings.copyrightEndTilNow>
					<settings.copyrightDateFormat>
						<TCEforms>
							<label>LLL:EXT:ipm_cline/Resources/Private/Language/locallang.xlf:flexform.copyrightDateFormat</label>
							<config>
								<type>input</type>
								<eval>required</eval>
								<default>d-m-Y</default>
								<placeholder>d-m-Y</placeholder>
								<max>5</max>
							</config>
						</TCEforms>
					</settings.copyrightDateFormat>
					<settings.bodytext>
						<TCEforms>
							<label>LLL:EXT:ipm_cline/Resources/Private/Language/locallang.xlf:flexform.bodytext</label>
								<config>
								  <type>text</type>
								  <cols>40</cols>
								  <rows>5</rows>
		                          <enableRichtext>1</enableRichtext>
								</config>
						</TCEforms>
					</settings.bodytext>
				</el>
			</ROOT>
		</main>
	</sheets>
</T3DataStructure>
